
; Old code 
(fn fibonacci (n)
    (if (= n 0)
      0
      (if (= n 1)
      1
      (+ (fibonacci (- n 1))
         (fibonacci (- n 2)))))) ;

(fn fibonacci (n)
(do (cond ((< n 1) 0)
        ((= n 1) 1)
        (true (+ (fibonacci (- n 1))
              (fibonacci (- n 2)))))))

(echo (fibonacci 7))
(echo (fibonacci 10))
